# CAPTURE DEVICE COMMAND #
uint8 LINE # LINE : 1, 2, 3, 4
uint8 CMD
# 00 : No Command
# 01 : CAPTURE DEVICE OFF
# 02 : CAPUTRE DEVICE ON

## CAPTURE DEVICE STATE ##
uint8 STATE
# 01 : START DONE
# 02 : START CHECKSUM ERROR
# 03 : START LINE ERROR
# 04 : START DISCHARGE ERROR
# 05 : START TIME ERROR
# 06 : STOP DONE
# 07 : STOP CHECKSUM ERROR
# 08 : WAITING FINISH
# 09 : FINISH
# 10 : REQUEST SAVED DATA
# 11 : RECEIVE SAVED DATA

int16 N_SAVE_DATA
int16 I_SAVE_DATA

int16[100] GET_LINE
uint64[100] GET_MSMT_TIME
int16[100] GET_FLOW_RATE
int16[100] GET_CAP_TIME
float32[100] GET_INTEG_FLOW_RATE
